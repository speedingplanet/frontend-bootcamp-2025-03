{
	"name": "react-app",
	"private": true,
	"version": "3.0.0",
	"type": "module",
	"author": "John Paxton <pax@speedingplanet.com>",
	"scripts": {
		"dev": "node scripts/check-labs.js && vite",
		"build": "tsc && vite build",
		"format": "prettier . --check || true",
		"format:write": "prettier . --write",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"setup": "node scripts/setup.js",
		"begin": "node scripts/setup.js begin",
		"finish": "node scripts/setup.js finish",
		"preview": "vite preview",
		"test": "vitest || true",
		"test:once": "vitest run",
		"tsc": "tsc -p tsconfig.json",
		"rest-server": "json-server -p 5100 data/bakery.json"
	},
	"dependencies": {
		"classnames": "^2.5.1",
		"nanoid": "^5.1.3",
		"react-toastify": "^11.0.5"
	},
	"description": "This template provides a minimal setup to get React working in Vite with HMR and some ESLint rules.",
	"main": "index.js",
	"directories": {
		"test": "tests"
	},
	"license": "ISC",
	"devDependencies": {
		"json-server": "^1.0.0-beta.3"
	}
}
